</html>
<script>
    $('.toggle-button').live("mouseover", function(){
        $(this).closest('.toggle-button').css('color','#77BACE');
    });
    $('.toggle-button').live("mouseout", function(){
        $(this).closest('.toggle-button').css('color','#999999');
    });
        
    $('.Navlist > li > a').live("mouseover", function(){
        $(this).closest('.Navlist > li > a').css('text-decoration','underline');
    });
    $('.Navlist > li > a').live("mouseout", function(){
        $(this).closest('.Navlist > li > a').css('text-decoration','none');
    });

    $('.header_logo').live('click', function(){
        location.replace('/main');
    });

    (function($) {

        var header_width = $('#header').width();
        var app = $.sammy(function() {
            this.use(Sammy.EJS);

            this.get('main#/', function() {
                $('#content').width((header_width-232)+14);
                var set_height = $('html').height();
                $('#content').height(set_height-66);
                $('#content').html("<div> main </div>");
            });

            this.get('#/admin/list', function() {
                $('#content').hide().load("main/admin/list", function(){
                    $('#content').show();
                    $('#content').height('100%');
                    $('#content').width((header_width-232));
                });
            });

            this.get('#/admin/enroll', function() {
                $('#content').hide().load("main/admin/enroll", function(){
                    $('#content').show();
                    $('#content').height('100%');
                    $('#content').width((header_width-232));
                });
            });

            this.get('#/admin/send', function() {
                $('#content').hide().load("main/admin/send", function(){
                    $('#content').show();
                    $('#content').height('100%');
                    $('#content').width((header_width-232));
                });
            });
        });

        $(function() {
            app.run('#/');
        });
    })(jQuery);
</script>