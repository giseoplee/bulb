<!DOCTYPE html>
<html>
	<head>
		<script src="//code.jquery.com/jquery-1.8.3.js"></script>
        <script src="/js/hideshow.js"></script>
    	<meta charset="UTF-8" />

    	<style>
    		html, body{
    			margin : 0;
    			padding : 0;
    			height: 100%
    		}
    		body{
    			display: table;
    			width: 100%;
    		}
    		#container{
    			width : 100%;
    			height : 100%;
    			display:table-row;
    		}
    		#header{
    			width : 100%;
    			height : 1px;
    			background-color: #2e2e2e;
    			display:table-row;
    		}
    		.logo_img_wrap{
                width : 231px;
    			text-align: center;
                border-right: solid 1px #A4A4A4;
                box-shadow: 3px 0px 5px #888888;
                display : inline-block;
    		}
    		.header_logo{
    			width : 110px;
    			height : 67px;
                padding-top: 7px;
                cursor : pointer;
    		}
    		.login_user{
                float : right;
                color : white;
                width : 135px;
                height : 50px;
                padding-top : 26px;
                font-size :13px;
    		}
            .login_user_text{
                position: relative;
                bottom: 9px;
                left: 6px;
            }
            .dashboard-text{
                float :right;
                height : 100%;
                font-weight: 900;
                font-size : 37px;
                padding-top : 22px;
                padding-right: 34%;
                display : inline-block;
                color : white;
            }
    		#Navigation{
    			width : 230px;
    			height : 100%;
    			float:left;
    			background-color: #D8D8D8;
    			border-right: solid 1px #A4A4A4;
    			z-index : 1;
                box-shadow: 3px 0px 5px #888888
    			
    		}
    		#content{
    			width: 100%;
    			margin :0 auto;
    			background-color: #F2F2F2;
    			height : 100%;
    		}
    		#footer{
    			display: table-row;
    			margin-left : 232px;
    			border-top : solid 1px #A4A4A4;
    			background-color: #BDBDBD;
    			height: 1px;
    			width : 100%;
    		}

            ul.Navlist{
                padding-left : 35px;
                list-style:none;
            }
            li{
                width : 100%;
            }
            a{
                text-decoration:none;
            }
            .Navcontent{
             
            }
            .icon_images{
                width : 30px;
            }
             .navtitle{
                font-size: 21px;
                padding-left : 20px;
            }
            .toggle-button{
                color : #999999;
                font-size : 12px;
                float : right;
                padding-top : 5px;
                padding-right: 10px;
                cursor : pointer;
            }
            .Navlist li{
                margin-bottom : 6px;
                font-size: 13px;
                color : #A4A4A4;
            }
            .Navlist li a{
                font-size: 13px;
                color : #585858;
            }
            .list_icon_size img{
                width : 15px;
            }
            .list_item_text{
                padding-left: 6px;
            }
    	</style>

	</head>

	<body>
		
			<div id="header">
				<div class="logo_img_wrap">
					<img class="header_logo" src="/images/bulb_logo.png" />
				</div>
                <div class="login_user">
                    <span>
                        <img class="icon_images" src="/images/user_login_1.png" />
                    <span> 
                    <span class="login_user_text"><%= User %></span>
                </div>
                <div class="dashboard-text">
                    Dashboard
                </div>
			</div>

			<div id="container">
				<div id="Navigation">
					<div class="NavContentAdmin">
                        <h3 class="navtitle">
                            <span>
                                ADMIN
                            </span>
                            <span class="toggle-button">
                                Hide
                            </span>
                        </h3>
						<ul class="Navlist">
                            <li>
                                <a href="#">
                                    <span class="list_icon_size">
                                        <img src="/images/list_applications.png" />
                                    <span>
                                    <span class="list_item_text">
                                        Applications List
                                    <span>
                                </a>
                            </li>
                            <li>
                                <a href="#">
                                    <span class="list_icon_size">
                                        <img src="/images/edit_applications.png" />
                                    <span>
                                    <span class="list_item_text">
                                        Edit Applications
                                    <span>
                                </a>
                            </li>
                            <li>
                                <a href="#">
                                    <span class="list_icon_size">
                                        <img src="/images/paper_plane.png" />
                                    <span>
                                    <span class="list_item_text">
                                        Send Notifications
                                    <span>
                                </a>
                            </li>
                        </ul>
					</div>
                    <div class="NavContentAdmin">
                        <h3 class="navtitle">
                            <span>
                                ADMIN
                            </span>
                            <span class="toggle-button">
                                Hide
                            </span>
                        </h3>
                        <ul class="Navlist">
                            <li>
                                <a href="#">
                                    <span class="list_icon_size">
                                        <img src="/images/list_applications.png" />
                                    <span>
                                    <span class="list_item_text">
                                        Applications List
                                    <span>
                                </a>
                            </li>
                            <li>
                                <a href="#">
                                    <span class="list_icon_size">
                                        <img src="/images/edit_applications.png" />
                                    <span>
                                    <span class="list_item_text">
                                        Edit Applications
                                    <span>
                                </a>
                            </li>
                            <li>
                                <a href="#">
                                    <span class="list_icon_size">
                                        <img src="/images/paper_plane.png" />
                                    <span>
                                    <span class="list_item_text">
                                        Send Notifications
                                    <span>
                                </a>
                            </li>
                        </ul>
                    </div>
				</div>

				<div id="content">
					contentPage
				</div>
                
			</div>
			
		</div>
	</body>
</html>
<script>
    $('.toggle-button').live("mouseover", function(){
        $(this).closest('.toggle-button').css('color','#77BACE');
    });
    $('.toggle-button').live("mouseout", function(){
        $(this).closest('.toggle-button').css('color','#999999');
    });
        
    $('.Navlist > li > a').live("mouseover", function(){
        $(this).closest('.Navlist > li > a').css('text-decoration','underline');
    });
    $('.Navlist > li > a').live("mouseout", function(){
        $(this).closest('.Navlist > li > a').css('text-decoration','none');
    });

    $('.header_logo').live('click', function(){
        location.replace('/main');
    });
</script>